/* pages/exampleShow/examplessearch/examplessearch.wxss */
page {
  background: #EFEFEF
}
.inputShow {
  background: white;
  padding:20rpx 0rpx;
}
.inputword {
  position: relative;
  font-size:32rpx;
}
.inputword input{
  width:92%;
  margin:0 auto;
  height:80rpx;
  line-height: 80rpx;
  background: #EFEFEF;
  border-radius: 10rpx;
  padding-left: 20rpx;
  padding-right: 150rpx;
  box-sizing: border-box;
}
.inputclear {
  position: absolute;
  right:40rpx;
  top:50%;
  transform: translateY(-50%);
  color:#4981FE;
}
.inputword image {
  width:40rpx;
  position: absolute;
  right:120rpx;
  top:50%;
  transform: translateY(-50%);
}
.history {
  background: white;
  border:1px solid #EFEFEF;
}
.historyimage {
  width:35rpx;
  display: inline-block;
  vertical-align: top;
  margin-right: 20rpx;
  position: relative;
  top:5rpx;
}
.historyword {
  display: inline-block;
  font-size:32rpx;
  word-break: break-all;
  width:430rpx;
}
.historyword text:nth-of-type(2){
  font-size: 26rpx;
  color:#929292;
  display: block;
  margin-top: 5rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.clearhistory {
  text-align: center;
  color:#888888;
  line-height: 100rpx;
  font-size:32rpx;
  background: transparent;
}
.item {
  padding:30rpx 40rpx;
  border-bottom: 1rpx solid #EFEFEF;
}
.mi {
  display: inline-block;
  font-size:26rpx;
  width:140rpx;
  line-height: 100%;
  color:#929292;
  margin-left:10rpx;
  bottom: 8rpx;
  position: relative;
  text-align: right;
}
.rightcover {
  position: fixed;
  right:14rpx;
  bottom:320rpx;
  text-align: right;
}
.bigcover {
  width:70rpx;
  height: 70rpx;
  background: #fff;
  padding:15rpx;
  box-sizing: border-box;
  margin: 0rpx auto 40rpx auto;
  box-shadow: 0rpx 0rpx 10rpx #ccc;
}
.bigcover > cover-image,
.myaddress > cover-image {
  width:100%;
  height:100%;
}
.myaddress {
  width:70rpx;
  height: 70rpx;
  background: #fff;
  padding:15rpx;
  box-sizing: border-box;
  border-radius: 50%;
  margin: 100rpx auto 0rpx auto;
  box-shadow: 0rpx 0rpx 10rpx #ccc;
}
.showinfo {
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  height: 180rpx;
  padding:30rpx;
  box-sizing: border-box;
  background: white;
}
.title {
  font-size:34rpx;
}
.des {
  font-size:30rpx;
  color:#929292;
  width:530rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-top:20rpx;
}

.gotoouther {
  width:100rpx;
  height: 100rpx;
  background: blue;
  padding:15rpx;
  box-sizing: border-box;
  border-radius: 50%;
  color:white;
  font-size:22rpx;
  box-shadow: 0rpx 0rpx 10rpx #ccc;
  text-align: center;
  position: absolute;
  right:30rpx;
  top:50%;
  transform: translateY(-50%);
}
.gotoouther image {
  width:40rpx;
  height:40rpx;
  margin: 0 auto;
  display: block;
}